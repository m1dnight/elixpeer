<div :if={@activity.loading}>Loading...</div>
<div :if={@activity.ok?}>
  <div id="speed-chart" phx-hook="SpeedChart"></div>
  <div id="volume-chart" phx-hook="VolumeChart"></div>
</div>
<div>
  <h1>Deleted</h1>
  <div>
    <%= for t <- @deleted do %>
      <.torrent_modal id={"torrent-modal-#{t.id}"} torrent={t} />
      <.torrent id={"torrent-#{t.id}"} torrent={t} read_only={true} />
    <% end %>
  </div>
</div>
